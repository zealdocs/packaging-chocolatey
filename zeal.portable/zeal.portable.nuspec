    <tags>zeal docs dev api admin</tags>
    <projectSourceUrl>https://github.com/zealdocs/zeal</projectSourceUrl>
    <bugTrackerUrl>https://github.com/zealdocs/zeal/issues</bugTrackerUrl>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
